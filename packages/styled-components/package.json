{
  "name": "@xstyled/styled-components",
  "description": "A utility-first CSS-in-JS framework built for React (styled-components bindings).",
  "version": "3.8.0",
  "keywords": [
    "styled-components",
    "css",
    "system",
    "styled-system",
    "tailwindcss"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "react-native": "./native/dist/index.cjs",
  "author": "Greg Berg√© <berge.greg@gmail.com>",
  "license": "MIT",
  "scripts": {
    "reset": "rm -rf dist && rm -rf native/dist",
